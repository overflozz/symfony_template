<script>
  $(document).ready(function() {
    $('select').material_select();
  });
  
    $('#loading-image').hide();


    {% for label, flashes in app.session.flashbag.all %}
        {% for flash in flashes %}
     
            
            Materialize.toast('{{ flash }}', 2000);
        {% endfor %}
    {% endfor %}

{% block javascript %}



    //Permet d'initialiser le link active du menu à celui de Home 
    $link_active = "#home";
    {% block script_menu_active %}
    
    //Permet de changer la valeur du link_active
    {% endblock %}
    //Permet de passer le link du menu en active selon la valeur de la page 
    $($link_active).addClass("active"); 


</script>

<script>
    $(".disapear").fadeOut(2000);
        
  $(document).ready(function(){
    // the "href" attribute of the modal trigger must specify the modal ID that wants to be triggered
      $('.modal').modal({
      dismissible: true, // Modal can be dismissed by clicking outside of the modal
      opacity: .5, // Opacity of modal background
      inDuration: 200, // Transition in duration
      outDuration: 200, // Transition out duration
      startingTop: '4%', // Starting top style attribute
      endingTop: '10%', // Ending top style attribute
      
  });
      
  });
  $(document).ready(function(){
    $('.tooltipped').tooltip({delay: 50});
  });


</script>


