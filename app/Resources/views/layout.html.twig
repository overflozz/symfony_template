    {% block menu %}
  </head>
  <body id="page-top" data-spy="scroll" data-target=".navbar-fixed-top">
    <!-- Navigation -->
    <div class="container-fluid  col-md-10 col-md-offset-2 col-sm-12">
        <nav class="navbar navbar-default navbar-fixed-top"  id="menuheader" role="navigation">
            <div class="container">
                <div class="navbar-header page-scroll">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand page-scroll" id="home"  href="#page-top">Visites EDF </a>
                </div>

                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse navbar-ex1-collapse">
                    <ul class="nav navbar-nav">
                        <!-- Hidden li included to remove active class from about link when scrolled up past about section -->
                        <li class="hidden">
                            <a class="page-scroll" href="#page-top"></a>
                        </li>
                        
                   
                        {% if is_granted("IS_AUTHENTICATED_REMEMBERED")  %}
                        <li>
                            <a class="page-scroll" id="calendar" href="{{path('of_calendar_homepage')}}"><span class="glyphicon glyphicon-calendar"></span> Calendrier</a>
                        </li>
                        <li>
                            <a class="page-scroll" id="faq" href="{{path('of_faq_homepage')}}"><span class="glyphicon glyphicon-question-sign"></span> FAQ</a>
                        </li>

                        <li>
                            <a class="page-scroll" id="document" href="{{path('of_calendar_voirDocuments')}}"><span class="glyphicon glyphicon-file"></span> Documents </a>

                        </li>
                        <li>
                            <a class="page-scroll" id="qualite" href="{{path('of_calendar_qualipanel')}}"><span class="glyphicon glyphicon-star"></span> Qualité </a>

                        </li>

                        {% endif %}
                        


                    </ul>
                    <!-- Lorsque le menu est affiché sur ordinateur ( on autorise le right float + menu deroulant) -->
                    <ul class="nav navbar-nav pull-right hidden-xs hidden-sm " id="userMenuDropdown">
                        
                        {% if is_granted("IS_AUTHENTICATED_REMEMBERED")  %}
                            
                            <li class="">
                                <a class="dropdown-toggle" id="username_dropdown" href="#" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="true"><img class="img-circle pull-left" src="{{ asset(app.user.getWebProfilePicturePath) }}" style="height : 30px; width:30px; margin-top:-5px; margin-right:5px;" alt=""> {{app.user.prenom}} <i class="material-icons pull-right" style="margin-top:-15px;">arrow_drop_down</i></a>
                                <ul class="dropdown-menu pull-right" id="logged_dropdown">
                                 

                                    <li><a href="{{ path('of_calendar_panelUser')}}"><span class="glyphicon glyphicon-list-alt"></span> Mes visites </a></li>
                                    <li><a href="{{ path('fos_user_profile_show') }}"><span class="glyphicon glyphicon-user"></span> Mon Profil</a></li>          
                                    <li><a href="{{ path('fos_user_profile_edit')}}"><span class="glyphicon glyphicon-cog"></span> Réglages Profil</a></li>
                                    <li><a href="{{ path('fos_user_security_logout') }}"><span class="glyphicon glyphicon-user"></span> Deconnexion</a></li> 
                                    {% if is_granted('ROLE_ADMIN') %} 

                            <li><a href="{{ path('of_calendar_statsAdmin') }}"><span class="glyphicon glyphicon-edit"></span> Administration</a></li>
                        {% endif %}

                                </ul>
                            </li>
                          

                        {% else %}
                            <li class="">
                                <a class="" href="{{ path('fos_user_profile_show') }}" data-toggle="" role="button" aria-haspopup="true" aria-expanded="true"><span class="glyphicon glyphicon-user"></span> Connexion <span class="caret"></span></a>
                                <ul class="dropdown-menu pull-right" id="login_dropdown">
                                <li></li>
                                        {{ render(controller('OFUserBundle:User:LoginMenu')) }}
                                   
                                </ul>
                            </li>

                        {% endif %}
                        
                    </ul>
                    <!-- Lorsque le menu est affiché sur mobile ( sm ), on enleve le float right -->
                    <ul class="nav navbar-nav visible-sm visible-xs mobileMenu">
                        
                        {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}

                           <li><a href="{{ path('of_calendar_panelUser')}}"><span class="glyphicon glyphicon-list-alt"></span> Mes visites </a></li>
                                    <li><a href="{{ path('fos_user_profile_show') }}"><span class="glyphicon glyphicon-user"></span> Mon Profil</a></li>          <li><a href="{{ path('fos_user_profile_edit')}}"><span class="glyphicon glyphicon-cog"></span> Réglages Profil</a></li>
                                    {% if is_granted('ROLE_ADMIN') %} 

                            <li><a href="{{ path('of_calendar_statsAdmin') }}"><span class="glyphicon glyphicon-edit"></span> Administration</a></li>
                        {% endif %}

                            <li><a href="{{ path('fos_user_security_logout') }}"><span class="glyphicon glyphicon-remove"></span> Déconnexion</a></li>

                        {% else %}

                            <li class=""><a href="{{ path('fos_user_security_login') }}">Connexion</a></li>

                        {% endif %}
                        
                    </ul>

                </div>
                <!-- /.navbar-collapse -->
